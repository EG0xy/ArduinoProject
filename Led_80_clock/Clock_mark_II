///===============================================================================

 t1=millis();
  if (t1-t2>=1){
    
     counter++;
    if(counter==9) counter=1;
 
  if (counter==1){
digitalWrite(1,HIGH);      
digitalWrite(2,HIGH);   
digitalWrite(3,HIGH);      
digitalWrite(4,HIGH);   
digitalWrite(5,HIGH);      
digitalWrite(6,HIGH);      
digitalWrite(7,HIGH);      
digitalWrite(8,HIGH);   
digitalWrite(9,HIGH);      
digitalWrite(10,HIGH); 

    digitalWrite(latchPin,LOW); 
    shiftOut(dataPin, clockPin, MSBFIRST ,B00000001);   
    digitalWrite(latchPin, HIGH);  

    digitalWrite(1, bitRead(ledx1, 7));
    digitalWrite(2, bitRead(ledx2, 7));   
    digitalWrite(3, bitRead(ledx3, 7));
    digitalWrite(4, bitRead(ledx4, 7));
    digitalWrite(5, bitRead(ledx5, 7));
    digitalWrite(6, bitRead(ledx6, 7));
    digitalWrite(7, bitRead(ledx7, 7));
    digitalWrite(8, bitRead(ledx8, 7));
    digitalWrite(9, bitRead(ledx9, 7));    
    digitalWrite(10, bitRead(ledx10, 7));       
  }   
  
  if (counter==2){  
digitalWrite(1,HIGH);      
digitalWrite(2,HIGH);   
digitalWrite(3,HIGH);      
digitalWrite(4,HIGH);   
digitalWrite(5,HIGH);      
digitalWrite(6,HIGH);      
digitalWrite(7,HIGH);      
digitalWrite(8,HIGH);   
digitalWrite(9,HIGH);      
digitalWrite(10,HIGH); 
    
    digitalWrite(latchPin,LOW); 
    shiftOut(dataPin, clockPin, MSBFIRST ,B00000010);     
    digitalWrite(latchPin, HIGH); 

    digitalWrite(1, bitRead(ledx1, 6));
    digitalWrite(2, bitRead(ledx2, 6));   
    digitalWrite(3, bitRead(ledx3, 6));
    digitalWrite(4, bitRead(ledx4, 6));
    digitalWrite(5, bitRead(ledx5, 6));
    digitalWrite(6, bitRead(ledx6, 6));
    digitalWrite(7, bitRead(ledx7, 6));
    digitalWrite(8, bitRead(ledx8, 6));
    digitalWrite(9, bitRead(ledx9, 6));    
    digitalWrite(10, bitRead(ledx10, 6));    
  }   

  if (counter==3){  

digitalWrite(1,HIGH);      
digitalWrite(2,HIGH);   
digitalWrite(3,HIGH);      
digitalWrite(4,HIGH);   
digitalWrite(5,HIGH);      
digitalWrite(6,HIGH);      
digitalWrite(7,HIGH);      
digitalWrite(8,HIGH);   
digitalWrite(9,HIGH);      
digitalWrite(10,HIGH);     
    digitalWrite(latchPin,LOW); 
    shiftOut(dataPin, clockPin, MSBFIRST ,B00000100);     
    digitalWrite(latchPin, HIGH);    

    digitalWrite(1, bitRead(ledx1, 5));
    digitalWrite(2, bitRead(ledx2, 5));   
    digitalWrite(3, bitRead(ledx3, 5));
    digitalWrite(4, bitRead(ledx4, 5));
    digitalWrite(5, bitRead(ledx5, 5));
    digitalWrite(6, bitRead(ledx6, 5));
    digitalWrite(7, bitRead(ledx7, 5));
    digitalWrite(8, bitRead(ledx8, 5));
    digitalWrite(9, bitRead(ledx9, 5));    
   digitalWrite(10, bitRead(ledx10, 5));   
  }   

  if (counter==4){ 
digitalWrite(1,HIGH);      
digitalWrite(2,HIGH);   
digitalWrite(3,HIGH);      
digitalWrite(4,HIGH);   
digitalWrite(5,HIGH);      
digitalWrite(6,HIGH);      
digitalWrite(7,HIGH);      
digitalWrite(8,HIGH);   
digitalWrite(9,HIGH);      
digitalWrite(10,HIGH);     
    digitalWrite(latchPin,LOW); 
    shiftOut(dataPin, clockPin, MSBFIRST ,B00001000); 
    digitalWrite(latchPin, HIGH);    

    digitalWrite(1, bitRead(ledx1, 4));
    digitalWrite(2, bitRead(ledx2, 4));   
    digitalWrite(3, bitRead(ledx3, 4));
    digitalWrite(4, bitRead(ledx4, 4));
    digitalWrite(5, bitRead(ledx5, 4));
    digitalWrite(6, bitRead(ledx6, 4));
    digitalWrite(7, bitRead(ledx7, 4));
    digitalWrite(8, bitRead(ledx8, 4));
    digitalWrite(9, bitRead(ledx9, 4));    
   digitalWrite(10, bitRead(ledx10, 4));           
  }   

  if (counter==5){   
digitalWrite(1,HIGH);      
digitalWrite(2,HIGH);   
digitalWrite(3,HIGH);      
digitalWrite(4,HIGH);   
digitalWrite(5,HIGH);      
digitalWrite(6,HIGH);      
digitalWrite(7,HIGH);      
digitalWrite(8,HIGH);   
digitalWrite(9,HIGH);      
digitalWrite(10,HIGH);     
    digitalWrite(latchPin,LOW); 
    shiftOut(dataPin, clockPin, MSBFIRST ,B00010000);      
    digitalWrite(latchPin, HIGH);  

    digitalWrite(1, bitRead(ledx1, 3));
    digitalWrite(2, bitRead(ledx2, 3));   
    digitalWrite(3, bitRead(ledx3, 3));
    digitalWrite(4, bitRead(ledx4, 3));
    digitalWrite(5, bitRead(ledx5, 3));
    digitalWrite(6, bitRead(ledx6, 3));
    digitalWrite(7, bitRead(ledx7, 3));
    digitalWrite(8, bitRead(ledx8, 3));
    digitalWrite(9, bitRead(ledx9, 3));    
   digitalWrite(10, bitRead(ledx10, 3));      
  }   

  if (counter==6){   
digitalWrite(1,HIGH);      
digitalWrite(2,HIGH);   
digitalWrite(3,HIGH);      
digitalWrite(4,HIGH);   
digitalWrite(5,HIGH);      
digitalWrite(6,HIGH);      
digitalWrite(7,HIGH);      
digitalWrite(8,HIGH);   
digitalWrite(9,HIGH);      
digitalWrite(10,HIGH);     
    digitalWrite(latchPin,LOW); 
    shiftOut(dataPin, clockPin, MSBFIRST ,B00100000); 
    digitalWrite(latchPin, HIGH); 

    digitalWrite(1, bitRead(ledx1, 2));
    digitalWrite(2, bitRead(ledx2, 2));   
    digitalWrite(3, bitRead(ledx3, 2));
    digitalWrite(4, bitRead(ledx4, 2));
    digitalWrite(5, bitRead(ledx5, 2));
    digitalWrite(6, bitRead(ledx6, 2));
    digitalWrite(7, bitRead(ledx7, 2));
    digitalWrite(8, bitRead(ledx8, 2));
    digitalWrite(9, bitRead(ledx9, 2));    
   digitalWrite(10, bitRead(ledx10, 2));   
  }   

  if (counter==7){
digitalWrite(1,HIGH);      
digitalWrite(2,HIGH);   
digitalWrite(3,HIGH);      
digitalWrite(4,HIGH);   
digitalWrite(5,HIGH);      
digitalWrite(6,HIGH);      
digitalWrite(7,HIGH);      
digitalWrite(8,HIGH);   
digitalWrite(9,HIGH);      
digitalWrite(10,HIGH);     
    digitalWrite(latchPin,LOW); 
    shiftOut(dataPin, clockPin, MSBFIRST ,B01000000);      
    digitalWrite(latchPin, HIGH);    

    digitalWrite(1, bitRead(ledx1, 1));
    digitalWrite(2, bitRead(ledx2, 1));   
    digitalWrite(3, bitRead(ledx3, 1));
    digitalWrite(4, bitRead(ledx4, 1));
    digitalWrite(5, bitRead(ledx5, 1));
    digitalWrite(6, bitRead(ledx6, 1));
    digitalWrite(7, bitRead(ledx7, 1));
    digitalWrite(8, bitRead(ledx8, 1));
    digitalWrite(9, bitRead(ledx9, 1));    
   digitalWrite(10, bitRead(ledx10, 1));    
  }   

  if (counter==8){  
    
digitalWrite(1,HIGH);      
digitalWrite(2,HIGH);   
digitalWrite(3,HIGH);      
digitalWrite(4,HIGH);   
digitalWrite(5,HIGH);      
digitalWrite(6,HIGH);      
digitalWrite(7,HIGH);      
digitalWrite(8,HIGH);   
digitalWrite(9,HIGH);      
digitalWrite(10,HIGH);     
    digitalWrite(latchPin,LOW); 
    shiftOut(dataPin, clockPin, MSBFIRST ,B10000000);    
    digitalWrite(latchPin, HIGH);   

    digitalWrite(1, bitRead(ledx1, 0));
    digitalWrite(2, bitRead(ledx2, 0));   
    digitalWrite(3, bitRead(ledx3, 0));
    digitalWrite(4, bitRead(ledx4, 0));
    digitalWrite(5, bitRead(ledx5, 0));
    digitalWrite(6, bitRead(ledx6, 0));
    digitalWrite(7, bitRead(ledx7, 0));
    digitalWrite(8, bitRead(ledx8, 0));
    digitalWrite(9, bitRead(ledx9, 0));    
   digitalWrite(10, bitRead(ledx10, 0));     
  }   
  

  t2=t1;
  }


 
