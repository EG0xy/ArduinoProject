// 80 LED clock 
// Low:E2  High:D9  Extend:FF atmega328p


#include "ClickButton.h"
#include <DS1302.h>
DS1302 rtc(A3, A4, A5);
Time t;

int latchPin = 13; 
int clockPin = 12; 
int dataPin = 11; 

ClickButton ba(A0, LOW, CLICKBTN_PULLUP);
ClickButton bb(A1, LOW, CLICKBTN_PULLUP);
ClickButton bc(A2, LOW, CLICKBTN_PULLUP);


int counter=1; // counter for multiplexing switch

unsigned long t1=0; // timer for multiplexing switch
unsigned long t2=0;

unsigned long t3=0;
unsigned long t4=0;


int setime=0;
int secx=1;
int minx=1;
int hrx=1;
int halfday=1;
int weekday=1;


byte  led1=B11111111;  //(1,11,21,31,41,51,61,71)  //switch for turning on 80 LEDS//active LOW
byte  led2=B11111111;  //(2,12,22,32,42,52,62,72)
byte  led3=B11111111;  //(3,13,23,33,43,53,63,73)
byte  led4=B11111111;  //(4,14,24,34,44,54,64,74)
byte  led5=B11111111;  //(5,15,25,35,45,55,65,75)
byte  led6=B11111111;  //(6,16,26,36,46,56,66,76)
byte  led7=B11111111;  //(7,17,27,37,47,57,67,77)
byte  led8=B11111111;  //(8,18,28,38,48,58,68,78)
byte  led9=B11111111;  //(9,19,29,39,49,59,69,79)
byte led10=B11111111; //(10,20,30,40,50,60,70,80)


byte  ledx1=B11111111;  //(1,11,21,31,41,51,61,71)  
byte  ledx2=B11111111;  //(2,12,22,32,42,52,62,72)
byte  ledx3=B11111111;  //(3,13,23,33,43,53,63,73)
byte  ledx4=B11111111;  //(4,14,24,34,44,54,64,74)
byte  ledx5=B11111111;  //(5,15,25,35,45,55,65,75)
byte  ledx6=B11111111;  //(6,16,26,36,46,56,66,76)
byte  ledx7=B11111111;  //(7,17,27,37,47,57,67,77)
byte  ledx8=B11111111;  //(8,18,28,38,48,58,68,78)
byte  ledx9=B11111111;  //(9,19,29,39,49,59,69,79)
byte ledx10=B11111111; //(10,20,30,40,50,60,70,80)

//==================================================================================

void setup() {
 
  pinMode(latchPin, OUTPUT);
  pinMode(clockPin, OUTPUT);
  pinMode(dataPin, OUTPUT);
  
 pinMode(1, OUTPUT);
 pinMode(2, OUTPUT);
 pinMode(3, OUTPUT);
 pinMode(4, OUTPUT);
 pinMode(5, OUTPUT);
 pinMode(6, OUTPUT);
 pinMode(7, OUTPUT);
 pinMode(8, OUTPUT);
 pinMode(9, OUTPUT);
 pinMode(10, OUTPUT);
   
digitalWrite(1,HIGH);      
digitalWrite(2,HIGH);   
digitalWrite(3,HIGH);      
digitalWrite(4,HIGH);   
digitalWrite(5,HIGH);      
digitalWrite(6,HIGH);      
digitalWrite(7,HIGH);      
digitalWrite(8,HIGH);   
digitalWrite(9,HIGH);      
digitalWrite(10,HIGH);      
    
  ba.multiclickTime = 50;
  bb.multiclickTime = 50; 
  bc.multiclickTime = 50; 
  
rtc.halt(false);
t = rtc.getTime();
 weekday=t.dow;
 hrx=t.hour%12;
 minx=t.min;
 secx=t.sec; 
  if(t.hour<12){ halfday=1;}  
  if(t.hour>11){halfday=2;}   


}

//==================================================================================

void loop() {
  
 
ba.Update();
bb.Update(); 
bc.Update();  
  
  
  
  if(ba.clicks==1) {
    halfday++;   
    setime=1;   
  }
  
  if(ba.clicks==-1) {
  }  
  
  
  if(bb.clicks==1) {
    hrx++; 
    setime=1;    
  }
  
  if(bb.clicks==-1) {
    weekday=1;
    halfday=1;
    hrx=1; 
    minx=1;
    secx=1;
    
    led1=B11111111; //(1,11,21,31,41,51,61,71)
    led2=B11111111; //(2,12,22,32,42,52,62,72)
    led3=B11111111; //(3,13,23,33,43,53,63,73)
    led4=B11111111; //(4,14,24,34,44,54,64,74)
    led5=B11111111; //(5,15,25,35,45,55,65,75)
    led6=B11111111; //(6,16,26,36,46,56,66,76)
    led7=B11111111; //(7,17,27,37,47,57,67,77)
    led8=B11111111; //(8,18,28,38,48,58,68,78)   
    led9=B11111111; //(9,19,29,39,49,59,69,79)    
   led10=B11111111; //(10,20,30,40,50,60,70,80) 
  
    setime=1; 
    
  }  
  
  
  if(bc.clicks==1) {
    minx++;
    setime=1;   
}

  if(bc.clicks==-1) {
  secx=1;
  setime=1;   
  }
  

 //===================================================================================================== 
  t3=millis(); // second timer
  if (t3-t4>=1000){
   secx++;  
  t4=t3;  
  }
   

   
  if(secx==30){
    t = rtc.getTime();
    secx=t.sec;  
}  

  if(secx==5){
    t = rtc.getTime();
     minx=t.min; 
     hrx=t.hour%12;
    if(t.hour<12){ halfday=1;}  
    if(t.hour>11){halfday=2;} 

    
}     



   
  if(secx==60){
  minx++; 
  secx=0;
}
  
  
  if(minx==60){
  hrx++; 
  minx=0;

    led1=led1|B11111111; //(1,11,21,31,41,51,61,71)
    led2=led2|B11111111; //(2,12,22,32,42,52,62,72)
    led3=led3|B11111111; //(3,13,23,33,43,53,63,73)
    led4=led4|B11111111; //(4,14,24,34,44,54,64,74)
    led5=led5|B11111111; //(5,15,25,35,45,55,65,75)
    led6=led6|B11111111; //(6,16,26,36,46,56,66,76)
    led7=led7|B11111111; //(7,17,27,37,47,57,67,77)
    led8=led8|B11111111; //(8,18,28,38,48,58,68,78)   
    led9=led9|B11111111; //(9,19,29,39,49,59,69,79)    
  led10=led10|B11111111; //(10,20,30,40,50,60,70,80)    

    ledx1=ledx1^B11111111; //(1,11,21,31,41,51,61,71)
    ledx2=ledx2^B11111111; //(2,12,22,32,42,52,62,72)
    ledx3=ledx3^B11111111; //(3,13,23,33,43,53,63,73)
    ledx4=ledx4^B11111111; //(4,14,24,34,44,54,64,74)
    ledx5=ledx5^B11111111; //(5,15,25,35,45,55,65,75)
    ledx6=ledx6^B11111111; //(6,16,26,36,46,56,66,76)
    ledx7=ledx7^B11111111; //(7,17,27,37,47,57,67,77)
    ledx8=ledx8^B11111111; //(8,18,28,38,48,58,68,78)   
    ledx9=ledx9^B11111111; //(9,19,29,39,49,59,69,79)    
  ledx10=ledx10^B11111111; //(10,20,30,40,50,60,70,80)    
  
}
  
  if(hrx==12){
halfday++;    
hrx=0;  


    led1=led1|B11111111; //(1,11,21,31,41,51,61,71)
    led2=led2|B11111111; //(2,12,22,32,42,52,62,72)
    led3=led3|B11111111; //(3,13,23,33,43,53,63,73)
    led4=led4|B11111111; //(4,14,24,34,44,54,64,74)
    led5=led5|B11111111; //(5,15,25,35,45,55,65,75)
    led6=led6|B11111111; //(6,16,26,36,46,56,66,76)
    led7=led7|B11111111; //(7,17,27,37,47,57,67,77)
    led8=led8|B11111111; //(8,18,28,38,48,58,68,78)   
    led9=led9|B11111111; //(9,19,29,39,49,59,69,79)    
  led10=led10|B11111111; //(10,20,30,40,50,60,70,80)    

    ledx1=ledx1^B11111111; //(1,11,21,31,41,51,61,71)
    ledx2=ledx2^B11111111; //(2,12,22,32,42,52,62,72)
    ledx3=ledx3^B11111111; //(3,13,23,33,43,53,63,73)
    ledx4=ledx4^B11111111; //(4,14,24,34,44,54,64,74)
    ledx5=ledx5^B11111111; //(5,15,25,35,45,55,65,75)
    ledx6=ledx6^B11111111; //(6,16,26,36,46,56,66,76)
    ledx7=ledx7^B11111111; //(7,17,27,37,47,57,67,77)
    ledx8=ledx8^B11111111; //(8,18,28,38,48,58,68,78)   
    ledx9=ledx9^B11111111; //(9,19,29,39,49,59,69,79)    
  ledx10=ledx10^B11111111; //(10,20,30,40,50,60,70,80)    
}

  if(halfday==3){
    weekday++;   
    halfday=1; 

    led1=led1|B11111111; //(1,11,21,31,41,51,61,71)
    led2=led2|B11111111; //(2,12,22,32,42,52,62,72)
    led3=led3|B11111111; //(3,13,23,33,43,53,63,73)
    led4=led4|B11111111; //(4,14,24,34,44,54,64,74)
    led5=led5|B11111111; //(5,15,25,35,45,55,65,75)
    led6=led6|B11111111; //(6,16,26,36,46,56,66,76)
    led7=led7|B11111111; //(7,17,27,37,47,57,67,77)
    led8=led8|B11111111; //(8,18,28,38,48,58,68,78)   
    led9=led9|B11111111; //(9,19,29,39,49,59,69,79)    
  led10=led10|B11111111; //(10,20,30,40,50,60,70,80)    

    ledx1=ledx1^B11111111; //(1,11,21,31,41,51,61,71)
    ledx2=ledx2^B11111111; //(2,12,22,32,42,52,62,72)
    ledx3=ledx3^B11111111; //(3,13,23,33,43,53,63,73)
    ledx4=ledx4^B11111111; //(4,14,24,34,44,54,64,74)
    ledx5=ledx5^B11111111; //(5,15,25,35,45,55,65,75)
    ledx6=ledx6^B11111111; //(6,16,26,36,46,56,66,76)
    ledx7=ledx7^B11111111; //(7,17,27,37,47,57,67,77)
    ledx8=ledx8^B11111111; //(8,18,28,38,48,58,68,78)   
    ledx9=ledx9^B11111111; //(9,19,29,39,49,59,69,79)    
  ledx10=ledx10^B11111111; //(10,20,30,40,50,60,70,80)     
}

  if(weekday==8){
  weekday=1;  


    led1=led1|B11111111; //(1,11,21,31,41,51,61,71)
    led2=led2|B11111111; //(2,12,22,32,42,52,62,72)
    led3=led3|B11111111; //(3,13,23,33,43,53,63,73)
    led4=led4|B11111111; //(4,14,24,34,44,54,64,74)
    led5=led5|B11111111; //(5,15,25,35,45,55,65,75)
    led6=led6|B11111111; //(6,16,26,36,46,56,66,76)
    led7=led7|B11111111; //(7,17,27,37,47,57,67,77)
    led8=led8|B11111111; //(8,18,28,38,48,58,68,78)   
    led9=led9|B11111111; //(9,19,29,39,49,59,69,79)    
  led10=led10|B11111111; //(10,20,30,40,50,60,70,80)    

    ledx1=ledx1^B11111111; //(1,11,21,31,41,51,61,71)
    ledx2=ledx2^B11111111; //(2,12,22,32,42,52,62,72)
    ledx3=ledx3^B11111111; //(3,13,23,33,43,53,63,73)
    ledx4=ledx4^B11111111; //(4,14,24,34,44,54,64,74)
    ledx5=ledx5^B11111111; //(5,15,25,35,45,55,65,75)
    ledx6=ledx6^B11111111; //(6,16,26,36,46,56,66,76)
    ledx7=ledx7^B11111111; //(7,17,27,37,47,57,67,77)
    ledx8=ledx8^B11111111; //(8,18,28,38,48,58,68,78)   
    ledx9=ledx9^B11111111; //(9,19,29,39,49,59,69,79)    
  ledx10=ledx10^B11111111; //(10,20,30,40,50,60,70,80)      
}
    
    
  if(setime==1){
    
    if(halfday==1){ 
    rtc.halt(false);
    rtc.writeProtect(false);  
    rtc.setTime(hrx, minx, secx);
    rtc.setDOW(weekday); } 
    
    if(halfday==2){ 
    rtc.halt(false);
    rtc.writeProtect(false); 
    rtc.setTime(hrx+12, minx, secx);    
    rtc.setDOW((weekday)); } 
    
    setime=0;    
  }
  

  
    
 //=====================================================================================================

 //=======================================================   




  //  B11111111
  //  B00000000
/*
  led1=B00000000; //(1,11,21,31,41,51,61,71)
  led2=B00000000; //(2,12,22,32,42,52,62,72)
  led3=B00000000; //(3,13,23,33,43,53,63,73)
  led4=B00000000; //(4,14,24,34,44,54,64,74)-
  led5=B00000000; //(5,15,25,35,45,55,65,75)
  led6=B00000000; //(6,16,26,36,46,56,66,76)
  led7=B00000000; //(7,17,27,37,47,57,67,77)
  led8=B00000000; //(8,18,28,38,48,58,68,78)   
  led9=B00000000; //(9,19,29,39,49,59,69,79)    
 led10=B00000000; //(10,20,30,40,50,60,70,80) 
 */
 
 
 

 switch (minx) {
    


    case 0:
    led1=led1&B01111111; //(1,11,21,31,41,51,61,71)
    led2=led2&B11111111; //(2,12,22,32,42,52,62,72)
    led3=led3&B11111111; //(3,13,23,33,43,53,63,73)
    led4=led4&B11111111; //(4,14,24,34,44,54,64,74)
    led5=led5&B11111111; //(5,15,25,35,45,55,65,75)
    led6=led6&B11111111; //(6,16,26,36,46,56,66,76)
    led7=led7&B11111111; //(7,17,27,37,47,57,67,77)
    led8=led8&B11111111; //(8,18,28,38,48,58,68,78)   
    led9=led9&B11111111; //(9,19,29,39,49,59,69,79)    
  led10=led10&B11111111; //(10,20,30,40,50,60,70,80)         
    break;
    
   
    
    case 1:
    led1=led1&B01111111; //(1,11,21,31,41,51,61,71)
    led2=led2&B01111111; //(2,12,22,32,42,52,62,72)
    led3=led3&B11111111; //(3,13,23,33,43,53,63,73)
    led4=led4&B11111111; //(4,14,24,34,44,54,64,74)
    led5=led5&B11111111; //(5,15,25,35,45,55,65,75)
    led6=led6&B11111111; //(6,16,26,36,46,56,66,76)
    led7=led7&B11111111; //(7,17,27,37,47,57,67,77)
    led8=led8&B11111111; //(8,18,28,38,48,58,68,78)   
    led9=led9&B11111111; //(9,19,29,39,49,59,69,79)    
  led10=led10&B11111111; //(10,20,30,40,50,60,70,80)     
    break;

    case 2:
    led1=led1&B01111111; //(1,11,21,31,41,51,61,71)
    led2=led2&B01111111; //(2,12,22,32,42,52,62,72)
    led3=led3&B01111111; //(3,13,23,33,43,53,63,73)
    led4=led4&B11111111; //(4,14,24,34,44,54,64,74)
    led5=led5&B11111111; //(5,15,25,35,45,55,65,75)
    led6=led6&B11111111; //(6,16,26,36,46,56,66,76)
    led7=led7&B11111111; //(7,17,27,37,47,57,67,77)
    led8=led8&B11111111; //(8,18,28,38,48,58,68,78)   
    led9=led9&B11111111; //(9,19,29,39,49,59,69,79)    
  led10=led10&B11111111; //(10,20,30,40,50,60,70,80)     
    break;

    case 3:
    led1=led1&B01111111; //(1,11,21,31,41,51,61,71)
    led2=led2&B01111111; //(2,12,22,32,42,52,62,72)
    led3=led3&B01111111; //(3,13,23,33,43,53,63,73)
    led4=led4&B01111111; //(4,14,24,34,44,54,64,74)
    led5=led5&B11111111; //(5,15,25,35,45,55,65,75)
    led6=led6&B11111111; //(6,16,26,36,46,56,66,76)
    led7=led7&B11111111; //(7,17,27,37,47,57,67,77)
    led8=led8&B11111111; //(8,18,28,38,48,58,68,78)   
    led9=led9&B11111111; //(9,19,29,39,49,59,69,79)    
  led10=led10&B11111111; //(10,20,30,40,50,60,70,80)     
    break;

    case 4:
    led1=led1&B01111111; //(1,11,21,31,41,51,61,71)
    led2=led2&B01111111; //(2,12,22,32,42,52,62,72)
    led3=led3&B01111111; //(3,13,23,33,43,53,63,73)
    led4=led4&B01111111; //(4,14,24,34,44,54,64,74)
    led5=led5&B01111111; //(5,15,25,35,45,55,65,75)
    led6=led6&B11111111; //(6,16,26,36,46,56,66,76)
    led7=led7&B11111111; //(7,17,27,37,47,57,67,77)
    led8=led8&B11111111; //(8,18,28,38,48,58,68,78)   
    led9=led9&B11111111; //(9,19,29,39,49,59,69,79)    
  led10=led10&B11111111; //(10,20,30,40,50,60,70,80)     
    break;

    case 5:
    led1=led1&B01111111; //(1,11,21,31,41,51,61,71)
    led2=led2&B01111111; //(2,12,22,32,42,52,62,72)
    led3=led3&B01111111; //(3,13,23,33,43,53,63,73)
    led4=led4&B01111111; //(4,14,24,34,44,54,64,74)
    led5=led5&B01111111; //(5,15,25,35,45,55,65,75)
    led6=led6&B01111111; //(6,16,26,36,46,56,66,76)
    led7=led7&B11111111; //(7,17,27,37,47,57,67,77)
    led8=led8&B11111111; //(8,18,28,38,48,58,68,78)   
    led9=led9&B11111111; //(9,19,29,39,49,59,69,79)    
  led10=led10&B11111111; //(10,20,30,40,50,60,70,80)     
    break;

    case 6:
    led1=led1&B01111111; //(1,11,21,31,41,51,61,71)
    led2=led2&B01111111; //(2,12,22,32,42,52,62,72)
    led3=led3&B01111111; //(3,13,23,33,43,53,63,73)
    led4=led4&B01111111; //(4,14,24,34,44,54,64,74)
    led5=led5&B01111111; //(5,15,25,35,45,55,65,75)
    led6=led6&B01111111; //(6,16,26,36,46,56,66,76)
    led7=led7&B01111111; //(7,17,27,37,47,57,67,77)
    led8=led8&B11111111; //(8,18,28,38,48,58,68,78)   
    led9=led9&B11111111; //(9,19,29,39,49,59,69,79)    
  led10=led10&B11111111; //(10,20,30,40,50,60,70,80)     
    break;

    case 7:
    led1=led1&B01111111; //(1,11,21,31,41,51,61,71)
    led2=led2&B01111111; //(2,12,22,32,42,52,62,72)
    led3=led3&B01111111; //(3,13,23,33,43,53,63,73)
    led4=led4&B01111111; //(4,14,24,34,44,54,64,74)
    led5=led5&B01111111; //(5,15,25,35,45,55,65,75)
    led6=led6&B01111111; //(6,16,26,36,46,56,66,76)
    led7=led7&B01111111; //(7,17,27,37,47,57,67,77)
    led8=led8&B01111111; //(8,18,28,38,48,58,68,78)   
    led9=led9&B11111111; //(9,19,29,39,49,59,69,79)    
  led10=led10&B11111111; //(10,20,30,40,50,60,70,80)     
    break;

    case 8:
    led1=led1&B01111111; //(1,11,21,31,41,51,61,71)
    led2=led2&B01111111; //(2,12,22,32,42,52,62,72)
    led3=led3&B01111111; //(3,13,23,33,43,53,63,73)
    led4=led4&B01111111; //(4,14,24,34,44,54,64,74)
    led5=led5&B01111111; //(5,15,25,35,45,55,65,75)
    led6=led6&B01111111; //(6,16,26,36,46,56,66,76)
    led7=led7&B01111111; //(7,17,27,37,47,57,67,77)
    led8=led8&B01111111; //(8,18,28,38,48,58,68,78)   
    led9=led9&B01111111; //(9,19,29,39,49,59,69,79)    
  led10=led10&B11111111; //(10,20,30,40,50,60,70,80)     
    break;


    case 9:
    led1=led1&B01111111; //(1,11,21,31,41,51,61,71)
    led2=led2&B01111111; //(2,12,22,32,42,52,62,72)
    led3=led3&B01111111; //(3,13,23,33,43,53,63,73)
